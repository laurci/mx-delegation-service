NODE_ENV=development # devnet | testnet | production
PORT=3005
PRIVATE_PORT=4002
KEYBASE_ENV=devnet # testnet | mainnet
NODE_CONFIG_DIR="apps/delegation-api/src/config"

# Enable the delegation API on this instance
DELEGATION_API_ENABLE=true

# Enable the transaction processor on this instance
TRANSACTION_PROCESSOR_ENABLE=true

#Redis for cache
REDIS_URL="localhost"
REDIS_PREFIX="development_env"
REDIS_PORT=6380
REDIS_DB=0

#Sentinel for Redis
SENTINEL_URL="localhost"
SENTINEL_PORT=26379
SENTINEL_NAME="localmaster"

#Elasticsearch server to use for logging. Optional
ELASTICSEARCH_URL=
ELASTICSEARCH_USERNAME=
ELASTICSEARCH_PASSWORD=

#Log filename to use for file logging. Optional
LOG_FILE=

#Log level can be one of : error, debug, info
LOG_LEVEL=debug

#Featured providers list goes here
FEATURED_PROVIDERS=elrond1,elrond2

# CORS domains
CORS_ORIGINS=/\.elrond\.com$/,/\.multiversx\.com$/,127.0.0.1,localhost

ELROND_INDEX=https://admin:admin@localhost:9200
ELROND_API=http://localhost:3001
ELROND_GATEWAY=http://127.0.0.1:7950
ELROND_EXPLORER=https://devnet-explorer.multiversx.com

# Keep Alive
KEEPALIVE_TIMEOUT_UPSTREAM=60000
KEEPALIVE_TIMEOUT_DOWNSTREAM=45000

#Inter API Communication RabbitMQ
INTER_API_RABBIT_MQ_URL=

#Github
GITHUB_TOKEN=

